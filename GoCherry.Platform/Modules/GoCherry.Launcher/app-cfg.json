{"profile" : "Classic",
 "version" : "1.0.0",
 "web" : {"httpPort" : 8080,
          "docRoot" : "www",
          "welcomeDoc": "index.html",
          "maxPostSize" : 2097152,
          "serverSessions" : true,
          "cookie" : {"name" : "JSESSIONID",
                      "domain" : "gocherry.org",
                      "path" : "/",
                      "timeToLive" : 60,
                      "version" : 1},
          "routing" : {"controllerNamespaces" : ["org.Cherry.Modules.Security.Agent", "org.Cherry.Go.Controllers"]}},
 "concurrency" : {"corePoolSize" : -1,
                  "maximumPoolSize" : -1,
                  "keepAliveTime" : -1},
 "mongo" : {"serverAddresses" : [{"host" : "gocherry.org", "port" : 27017}],
            "dbName" : "test"},
 "security" : {"crypto" : {"digest" : {"algorithm" : "SHA-256", "saltSizeBytes" : 8, "iterations" : 13, "poolSize" : 8}}},
 "hazelcast" : {"configFile" : "hazelcast.xml"}}
